<ResourceDictionary x:Class="JaStDev.HAB.Designer.TestCaseDictionary"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ctrls="clr-namespace:JaStDev.HAB.Designer.WPF.Controls">


    <Style x:Key="TrvPanelItemTestCaseItemStyle"
           TargetType="{x:Type ctrls:TreeViewPanelItem}"
           BasedOn="{StaticResource {x:Type ctrls:TreeViewPanelItem}}">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <!--Template for text channels drop down box.-->
    <DataTemplate x:Key="TestCaseTextChannelTemplate">
        <TextBlock Text="{Binding Path=NeuronInfo.DisplayTitle}" />

    </DataTemplate>
</ResourceDictionary>